<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>ProyectoBaseMaven</display-name>
	<servlet>
		<description></description>
		<display-name>DoProcess</display-name>
		<servlet-name>DoProcess</servlet-name>
		<servlet-class>co.jankins.servlet.DoProcess</servlet-class>
	</servlet>
	<servlet>
		<display-name>InitDataSource</display-name>
		<servlet-name>InitDataSource</servlet-name>
		<servlet-class>co.jankins.datasource.InitDataSource</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet>
		<display-name>InitJankerMonitor</display-name>
		<servlet-name>InitJankerMonitor</servlet-name>
		<servlet-class>co.jankins.monitor.InitMonitor</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>DoProcess</servlet-name>
		<url-pattern>/DoProcess</url-pattern>
	</servlet-mapping>
	
	<filter>
		<filter-name>safeRequestParameters</filter-name>
		<filter-class>co.jankins.security.filters.SecurityParameterFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>safeRequestParameters</filter-name>
		<url-pattern>/DoProcess</url-pattern>
	</filter-mapping>

</web-app>